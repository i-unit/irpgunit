         </action>
         <action
               label="%SaveAsText"
               class="de.tools400.rpgunit.isphere.action.SpooledFileSaveAsTextAction"
               menubarPath="biz.isphere.rpgunit.unittestsuite.report.save/main"
               enablesFor="+"
               id="de.tools400.rpgunit.isphere.action.SpooledFileSaveAsTextAction">
         </action>
         <action
               label="%OpenAsPDF"
               class="de.tools400.rpgunit.isphere.action.SpooledFileOpenAsPDFAction"
               menubarPath="biz.isphere.rpgunit.unittestsuite.report.open/main"
               enablesFor="+"
               id="de.tools400.rpgunit.isphere.action.SpooledFileOpenAsPDFAction">
         </action>
         <action
               label="%OpenAsHTML"
               class="de.tools400.rpgunit.isphere.action.SpooledFileOpenAsHTMLAction"
               menubarPath="biz.isphere.rpgunit.unittestsuite.report.open/main"
               enablesFor="+"
               id="de.tools400.rpgunit.isphere.action.SpooledFileOpenAsHTMLAction">
         </action>
         <action
               label="%OpenAsText"
               class="de.tools400.rpgunit.isphere.action.SpooledFileOpenAsTextAction"
               menubarPath="biz.isphere.rpgunit.unittestsuite.report.open/main"
               enablesFor="+"
               id="de.tools400.rpgunit.isphere.action.SpooledFileOpenAsTextAction">
         </action>
	  </objectContribution>
	  
   </extension>

</plugin>
